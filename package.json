{
  "name": "coffeescript-formatter",
  "displayName": "CoffeeScript Formatter",
  "description": "CoffeeScript formatter based on coffee-fmt package.",
  "version": "0.0.1",
  "publisher": "prplx",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.format"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.format",
        "title": "Format CoffeeScript"
      }
    ],
    "keybindings": {"command": "extension.format", "key": "ctrl+shift+f", "mac": "cmd+shift+f", "when": "editorTextFocus"}
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "eslint": "^3.6.0",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "coffee-fmt": "^0.12.0"
  }
}
